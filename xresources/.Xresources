! Rofi
rofi.font: Fira Code 16
! State:           bg      border  separator
rofi.color-window: #222222,   #FF00D9,   #222222
! State:           'bg',          'fg',    'bgalt',       'hlbg',        'hlfg'
rofi.color-normal: #191919, #f1ebeb, argb:00000000, #FF00D9, #FFFFFF
rofi.color-active: #191919, #f1ebeb, argb:00000000, #FF00D9, #FFFFFF
rofi.color-urgent: #191919, #f1ebeb, argb:00000000, #FF00D9, #FFFFFF
rofi.separator-style: none
rofi.hide-scrollbar: true
rofi.fullscreen: false
rofi.line-margin: 0
rofi.eh: 1
rofi.bw: 4
rofi.width: 30
rofi.location: 0
rofi.columns: 1
rofi.padding: 15
rofi.lines: 10

! SXIV
Sxiv.font: Fira Code:size=13
Sxiv.background: #030303
Sxiv.foreground: #ffffff

!xrdb -merge ~/.Xresources
